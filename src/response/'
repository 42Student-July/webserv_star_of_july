#include "CGIParser.hpp"

CGIParser::CGIParser(std::string cgi_response)
	:cgi_response_(cgi_response)
{
}

CGIParser::~CGIParser()
{
}

void CGIParser::run()
{
	parseHeader();
	
}

void CGIParser::parseHeader()
{
	size_t body_start = 0;
	while (true) {
		body_start = cgi_response_.find("\r\n");
		std::string header = cgi_response_.substr(0, body_start)
	}
}

CGIParser::CGIParser(CGIParser const &other)
{
    *this = other;
}

CGIParser &CGIParser::operator=(CGIParser const &other)
{
    if (this != &other)
    {
    }
    return *this;
}
